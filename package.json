  "bugs": {
    "url": "https://github.com/wilsonzlin/cabinet/issues"
  },
  "engines": {
    "node": ">=7"
  },
  "homepage": "https://github.com/wilsonzlin/cabinet#readme",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "extlib": "^0.0.3",
    "image-size": "^0.8.3",
    "mdn-browser-compat-data": "0.0.98",
    "mime": "^2.4.4",
