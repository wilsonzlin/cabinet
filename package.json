    "@types/express": "^4.17.6",
    "@types/mime": "^2.0.1",
    "@types/minimist": "^1.2.0",
    "@types/mkdirp": "^1.0.0",
    "@types/node": "^13.11.1",
    "@types/progress": "^2.0.3",
