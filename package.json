{
  "name": "@wzlin/cabinet",
  "version": "0.0.16",
  "description": "Zero-configuration video + photo server and app",
  "bin": "build/main.js",
  "files": [
    "build/**"
  ],
  "scripts": {
    "build": "npm run clean && tsc && chmod +x build/main.js && npm run buildClient",
    "buildClient": "node compile/client.js",
    "clean": "rm -rf build",
    "prepublishOnly": "npm run build",
    "run": "npm run build && node build/main.js",
    "start": "ts-node src/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wilsonzlin/cabinet.git"
  },
