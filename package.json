{
  "name": "@wzlin/cabinet",
  "version": "0.0.1",
  "description": "Command-line zero-configuration photo/video HTTP server and client",
  "bin": "build/main.js",
  "files": [
    "build/**"
  ],
  "scripts": {
    "build": "npm run clean && tsc && cp -r src/client build/client",
    "clean": "rm -rf build",
    "prepublishOnly": "npm run build",
    "start": "ts-node -r tsconfig-paths/register src/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wilsonzlin/cabinet.git"
  },
  "author": {
    "email": "npm@wilsonl.in",
    "name": "Wilson Lin",
    "url": "https://wilsonl.in/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wilsonzlin/cabinet/issues"
  },
  "homepage": "https://github.com/wilsonzlin/cabinet#readme",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "image-size": "^0.8.3",
    "mime": "^2.4.4",
    "minimist": "^1.2.0",
    "pug": "^2.0.4",
    "readdirp": "^3.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.1",
    "@types/mime": "^2.0.1",
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.11.1",
    "@types/pug": "^2.0.4",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.6.4"
  }
}
